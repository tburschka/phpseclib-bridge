{
  "name": "tburschka/phpseclib-bridge",
  "description": "Bridge to simplify usage of phpseclib",
  "license": "MIT",
  "keywords": [
    "php",
    "phpseclib",
    "ssh",
    "scp",
    "sftp"
  ],
  "homepage": "https://github.com/tburschka/phpseclib-bridge",
  "require": {
    "php": ">=5.3",
    "phpseclib/phpseclib": "~0.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "autoload": {
    "psr-4": {
      "phpseclibBridge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "phpseclibBridge\\Tests\\": "tests/"
    }
  }
}